# Environment Configuration Example
# Copy this file to .env in the project root and update the values

# Application Environment
VITE_APP_ENV=development
VITE_APP_VERSION=1.0.0
VITE_APP_NAME=Baltek Jobs

# API Configuration
VITE_API_BASE_URL=https://api.baltek.net/api

# Authentication
VITE_OAUTH_AUTH_URL=https://api.baltek.net/api/oauth2/authorize/
VITE_OAUTH_CLIENT_ID=your_oauth_client_id
VITE_OAUTH_TOKEN_URL=https://api.baltek.net/api/oauth2/token/
VITE_OAUTH_REDIRECT_URI=https://localhost:5001/auth/callback

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_TRACKING=false

# Development
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug

# Production Environment (uncomment for production)
# VITE_APP_ENV=production
# VITE_DEBUG_MODE=false
# VITE_LOG_LEVEL=error
# VITE_ENABLE_ANALYTICS=true
# VITE_ENABLE_ERROR_TRACKING=true
# VITE_OAUTH_REDIRECT_URI=https://your-domain.com/auth/callback
